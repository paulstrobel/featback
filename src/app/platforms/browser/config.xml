<?xml version='1.0' encoding='utf-8'?>
<widget id="collex.education" version="1.0.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Collex</name>
    <description>Collex Education - Erlebe deinen Traumberuf</description>
    <author email="info@collex.education" href="http://collex.education">Paul Strobel</author>
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-spinner" source="npm" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="11" />
    <preference name="android-targetSdkVersion" value="14" />
    <preference name="orientation" value="landscape" />
</widget>
